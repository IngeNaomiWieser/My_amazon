

<% if @user %>
  <h1><%= @user.first_name %>'s Liked Questions</h1>
<% else %>
  <h1>All Questions</h1>
<% end %>

<% @products.each do |product| %>
  <div>
    <h2><%= link_to product.title, product_path(product) %></h2>
    <!-- <p> <%=product.description  %></p>
    <p> <%=product.price %></p> -->
    <hr>
  </div>
<% end %>
